<p id="notice"><%= notice %></p>

<h1>Listing Photos</h1>

<table>
  <thead>
    <tr>
      <th>Path name</th>
      <th>File name</th>
      <th>Md5</th>
      <th>Perceptual hash</th>
      <th>Status</th>
      <th>Width</th>
      <th>Height</th>
      <th>Color</th>
      <th>File created at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.path_name %></td>
        <td><%= photo.file_name %></td>
        <td><%= photo.md5 %></td>
        <td><%= photo.perceptual_hash %></td>
        <td><%= photo.status %></td>
        <td><%= photo.width %></td>
        <td><%= photo.height %></td>
        <td><%= photo.color %></td>
        <td><%= photo.file_created_at %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
